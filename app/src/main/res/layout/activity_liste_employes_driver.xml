<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".liste_employes_driver">

    <ImageView
        android:id="@+id/menu"
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:contentDescription="@string/menu"
        android:src="@drawable/menu"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ImageContrastCheck" />

    <TextView
        android:id="@+id/liste_des_s"
        android:layout_width="140dp"
        android:layout_height="18dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppins"
        android:text="@string/liste_des_s"
        android:textAppearance="@style/historique_"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu" />


    <Button
        android:id="@+id/bar"
        android:layout_width="150dp"
        android:layout_height="2dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/line"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/liste_des_e"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/liste_des_e"
        android:layout_width="160dp"
        android:layout_height="18dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:fontFamily="@font/poppins_bold"
        android:gravity="center"
        android:text="@string/liste_des_e"
        android:textAppearance="@style/trajet_en_c"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu"
        tools:ignore="TextContrastCheck" />


    <Spinner
        android:id="@+id/stations_spinner"
        app:layout_constraintTop_toBottomOf="@+id/liste_des_s"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp" />

    <Button
        android:id="@+id/emp1"
        android:background="@drawable/jixo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stations_spinner"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>

    <Button
        android:id="@+id/emp2"
        android:background="@drawable/jixo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stations_spinner"
        android:layout_marginTop="50dp"
        android:layout_marginStart="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>

    <Button
        android:id="@+id/emp3"
        android:background="@drawable/jixo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp2"
        android:layout_marginTop="30dp"
        android:layout_marginStart="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>


    <Button
        android:id="@+id/emp4"
        android:background="@drawable/jixo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp2"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>


    <Button
        android:id="@+id/emp5"
        android:background="@drawable/jixo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp3"
        android:layout_marginTop="30dp"
        android:layout_marginStart="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>


    <Button
        android:id="@+id/emp6"
        android:background="@drawable/jixo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp4"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>


    <Button
        android:id="@+id/emp7"
        android:background="@drawable/jixo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp5"
        android:layout_marginTop="30dp"
        android:layout_marginStart="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>


    <Button
        android:id="@+id/emp8"
        android:background="@drawable/jixo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp6"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>

    <Button
        android:id="@+id/emp9"
        android:background="@drawable/jixo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp7"
        android:layout_marginTop="30dp"
        android:layout_marginStart="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>


    <Button
        android:id="@+id/emp10"
        android:background="@drawable/jixo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp8"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>


    <Button
        android:id="@+id/emp11"
        android:background="@drawable/jixo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp9"
        android:layout_marginTop="30dp"
        android:layout_marginStart="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>



    <Button
        android:id="@+id/emp12"
        android:background="@drawable/jixo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emp10"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="50dp"
        android:layout_width="120dp"
        android:layout_height="35dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>